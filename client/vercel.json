
  {
    "version": 2,
    "builds": [
      {
        "src": "frontend/**",
        "use": "@vercel/static"
      }
    ],
    "proxies": [
      {
        "src": "/api/(.*)",
        "dest": "https://klcollection-backend.onrender.com/api/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  